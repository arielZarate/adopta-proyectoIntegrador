{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true, //true
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/Landing.tsx",
    "src/components/Layout/Navbar.jsx",
    "src/context/PetsContext.js",
    "src/components/FilterForm.jsx",
    "src/context/PetContext_version1.txtNoBorrar",
    "src/hooks/PetFiltertHook 2ts",
    "src/hooks/PetFiltertHook_version1.txtNoBorrar"
  ],
  "exclude": ["node_modules"]
}
